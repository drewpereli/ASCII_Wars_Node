#debug-container
	.btn.btn-default.btn-lg#restart-btn Restart
	.btn.btn-default.btn-lg#raise-elevation-btn Raise Elevation
	.btn.btn-default.btn-lg#lower-elevation-btn Lower Elevation
script.
	$('#restart-btn').click(() => {
		app.socket.emit('restart');
	});

	$('#raise-elevation-btn').click(() => {
		app.game.changeState('raising elevation');
	});

	$('#lower-elevation-btn').click(() => {
		app.game.changeState('lowering elevation');
	});